<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="likes">

	<select id="selectList" resultType="Likes" parameterType="Likes">
		SELECT no, type, user_no userNo, contents_no contentsNo, pros_cons prosCons, regdate
		FROM likes
	</select>
	
</mapper>  










